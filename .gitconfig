[user]
	name = Ben Sedat
	username = bsedat
	email = bsedat@tinfoilsecurity.com
	signingkey = C8FF2591140CAE47
[core]
	excludesfile = /Users/bsedat/.gitignore_global
	ignorecase = false
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	publish = !git push origin $(git rev-parse --abbrev-ref HEAD):$(whoami)/$(git rev-parse --abbrev-ref HEAD) -u
	cleanup-local = !git branch --merged | grep -v '*' | xargs -n 1 git branch -d
	cleanup-origin = !git branch --remote --list origin* --merged | grep -v 'origin/master$' | sed 's/origin\\///' | xargs -n 1 -I BRANCH git push origin :BRANCH
	cleanup = !git cleanup-local && git cleanup-origin
	set-upstream = branch --set-upstream-to=upstream/master
	stash-all = stash save --include-untracked
[color]
	ui = auto
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[push]
	default = simple
	followTags = true
[help]
	autocorrect = 15
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[rebase]
	autostash = true
	autosquash = true
[stash]
	showPatch = true
[fetch]
	prune = true
[diff]
	compactionHeuristic = true
[commit]
	verbose = true
	gpgsign = true
[status]
	showUntrackedFiles = all
[credential]
	helper = osxkeychain
[submodule]
	recurse = true
