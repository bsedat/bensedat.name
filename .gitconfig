[user]
	name = Ben Sedat
	email = bsedat@tinfoilsecurity.com
	signingkey = 3B22DAD2
[core]
	excludesfile = /Users/bsedat/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	wtf = !git-wtf
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	publish = !git push upstream $(git rev-parse --abbrev-ref HEAD):bsedat/$(git rev-parse --abbrev-ref HEAD) -u
[color]
	ui = auto
